        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textview_dat"
        app:layout_constraintTop_toBottomOf="@+id/textview_pric" />
-->




</androidx.constraintlayout.widget.ConstraintLayout>